#include <iostream>
using namespace std;
int main() {
    int number, sum = 0;
    cout << "Enter a positive integer: ";
    cin >> number;
    for (int i = 1; i < number; i++) {
        if (number % i == 0) {
            sum += i;
        }
    }
    if (sum == number)
        cout << number << " is a Perfect number.";
    else
        cout << number << " is not a Perfect number.";
    return 0;
}
