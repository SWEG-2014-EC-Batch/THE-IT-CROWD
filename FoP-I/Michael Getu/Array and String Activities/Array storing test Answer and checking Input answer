#include<iostream>
#include<string.h>
using namespace std;

int main() {
    int count =0;
    char testAns[]= {'a','b','c','d','d'};
    char Rans[5];
    for (int i=0; i<5; i++) {
    cout << "Enter your mark for question "<<i+1<<": ";
    cin >> Rans[i];
     }
    cout << "\t Result"<<endl;
    for (int i=0; i<5; i++) {
        
    if (Rans[i]==testAns[i]) {
            cout <<"Question "<<i+1<< ": Correct"<<endl;
        count++;
        }
        else 
            cout <<"Question "<<i+1 <<": Wrong"<<endl;
    }
            cout << "You have got " <<count << " out of 5"<<endl;

   
}
