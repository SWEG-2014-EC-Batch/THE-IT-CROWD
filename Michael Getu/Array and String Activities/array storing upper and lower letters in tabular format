#include<iostream>
using namespace std;
     // Program that shows the upper and lower letter sets with 4 colomns using an array to store the values
int main() 
{
    
    int symb [26] = {65,66,67,68,69,70,71,72,73,74,75,76,77,78,79,80,81,82,83,84,85,86,87,88,89,90};
    for (int i =0; i<4; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    } 
    cout << endl;
    for (int i =4; i<8; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    }
    cout << endl;
    for (int i =8; i<12; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    }
    cout << endl;
    for (int i =12; i<16; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    }
    cout << endl;
    for (int i =16; i<20; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    }
    cout << endl;
    for (int i =20; i<24; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    }
    cout << endl;
    for (int i =24; i<26; i++) {
        cout << (char)(tolower(symb[i]))<<(char)(toupper(symb[i]))<<"\t";
    }
    cout << endl;
   
return 0;
}
