#include<iostream>
using namespace std;

struct coordinate{
   double x,y;
};

coordinate readme(){
   coordinate point;
   cout<<"enter the coordinate :"<<endl;
   cin>>point.x>>point.y;

  return point; 
}

void print(coordinate num){
    cout<<"the coordinate = ("<<num.x<<","<<num.y<<")"<<endl;
}

int main(){
  
   print(readme());

  return 0;  
}
