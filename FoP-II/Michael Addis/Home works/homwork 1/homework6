#include <iostream>
#include <cmath>
using namespace std;

double hypotenuse(double num1, double num2)
{

    return sqrt((pow(num1, 2)) + pow(num2, 2));
}
int main()
{
    double side1, side2;
    cout << "Enter the first side: ";
    cin >> side1;
    cout << "Enter the second side: ";
    cin >> side2;
    cout << "The hypotenuse is " << hypotenuse(side1, side2) << endl;
    return 0;
}
